public class JvmComprehension {

 public static void main(String [] args) {
 int i = 1; // 1 сохраняется в тек потока потомучто примитивный тип ; насколько я понял сдесь походят все ClassLoader по обычной схеме; 
        Object o = new Object(); // 2 сохраняется в кучу  потомучто обект а сылка кидается в стек ; насколько я понял сдесь походят все ClassLoader по обычной схеме
        Integer ii = 2; // 3 это обертка обекта и она тоже сохраняется в кучу но ее сылка кидается в стек; насколько я понял сдесь походят все ClassLoader по обычной схеме
        printAll(o, i, ii); // 4 тут создается новый поток и мы там создаем новые сылки которые будут сылаться на тотже обект а int создадится заново; насколько я понял сдесь походят все ClassLoader по обычной схеме; метод окончен его данные никуда дальше не идут и сылок нет сборщик мусора его удаляет
        System.out.println("finished"); // 7 вызов методов System.out.println тут ничего не сохраняется и происходит только обычная загрузка и вывод текста
 }

 private static void printAll (Object o, int i, Integer ii) {
 Integer uselessVar = 700; // 5  это обертка и она сохраняется в кучу но ее сылка кидается в стек ; насколько я понял сдесь походят все ClassLoader по обычной схеме
        System.out.println(o.toString() + i + ii); // 6 вызов методов System.out.println тут ничего не сохраняется и происходит только обычная загрузка и простые вычисления с их выводом
}
}
